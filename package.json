{
  "name": "trello-manager",
  "displayName": "Trello Manager",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Trello Manager",
      "properties": {
        "trello-manager.username": {
          "type": "string",
          "description": "Votre nom d'utilisateur Trello",
          "scope": "application"
        },
        "trello-manager.apiKey": {
          "type": "string",
          "description": "Votre cl√© d'API Trello",
          "scope": "application"
        },
        "trello-manager.apiToken": {
          "type": "string",
          "description": "Votre token d'API Trello",
          "scope": "application"
        }
      }
    },
    "commands": [
      {
        "command": "trello-manager.authenticate",
        "title": "Authenticate Trello"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "axios": "^0.19.2"
  }
}
